<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>FreeImageNET</id>
        <version>3.15.1</version>
        <title>FreeImageNET</title>
        <authors>FreeImage developers</authors>
        <owners>chefkoch</owners>
        <licenseUrl>http://freeimage.sourceforge.net/license.html</licenseUrl>
        <projectUrl>http://freeimage.sourceforge.net/</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>FreeImage is an Open Source library project for developers who would like to support popular graphics image formats like PNG, BMP, JPEG, TIFF and others as needed by today's multimedia applications. FreeImage is easy to use, fast, multithreading safe, compatible with all 32-bit or 64-bit versions of Windows, and cross-platform (works both with Linux and Mac OS X).</description>
        <summary>Contains a .NET wrapper for FreeImage.dll.</summary>
        <dependencies>
            <group targetFramework=".NETFramework2.0">
                <dependency id="FreeImage" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="lib\net20\FreeImageNET.dll" target="lib\net20\FreeImageNET.dll" />
    </files>
</package>