    <metadata>
        <id>BASS</id>
        <version>2.4.10</version>
        <title>BASS Core (bass.dll)</title>
        <authors>un4seen Developments</authors>
        <owners>chefkoch</owners>
        <licenseUrl>http://www.un4seen.com/bass.html#license</licenseUrl>
